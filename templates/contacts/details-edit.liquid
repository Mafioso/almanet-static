      <div class="stream-item">
        <div class="row">
          <div class="row-icon">
            [i]
          </div>
          <div class="row-body row-body--no-trailer">
            <div class="row">
              <a href="#" class="row-icon">
                <figure class="icon-userpic"></figure>
              </a>
              <div class="row-body">
                <div class="row">
                  <div class="row-body-primary text-caption text-secondary">
                    <a href="#" class="text-secondary">Абиш</a> в 10:06, 7 окт 14
                  </div>
                  <div class="row-body-secondary">
                    <a href="#" class="link-inline">
                      [c]
                    </a>
                  </div>
                </div>
